   // Reverse ArrayList
public static List<Integer> arrayReverse(List<Integer> a){
        List<Integer> list = new ArrayList<>();
        for (int i=a.size()-1;i>=0;i--){
            list.add(a.get(i));
        }
        return list;
    }
    //Just method :D 
    
